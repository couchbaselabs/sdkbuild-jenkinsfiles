        stage('package') {
            agent { label 'centos7' }
            steps {
                cleanWs()
                sh('sudo yum install -y perl-XML-XPath')
                sh('cbdep install --dir /tmp/php php-zts 7.3.13-cb1')
                sh('/tmp/php/php-zts-7.3.13-cb1/bin/php --version')
                sh('/tmp/php/php-zts-7.3.13-cb1/bin/pear version')
                unstash 'php-couchbase'
                dir('php-couchbase') {
                    sh('sed -i   "s:^    <date>.*</date>:<date>\$(date +%Y-%m-%d)</date>:" package.xml')
                    sh('/tmp/php/php-zts-7.3.13-cb1/bin/pear package')
                    archiveArtifacts(artifacts: "couchbase-*.tgz", fingerprint: true)
                    stash(includes: 'couchbase-*tgz', name: 'tarball', useDefaultExcludes: false)
                    sh('tar xf couchbase-*.tgz')
                }
                unstash 'php_couchbase-7.3.13-zts-x64'
                sh("""
cp php-couchbase/{package.xml,LICENSE} php_couchbase-7.3.13-zts-x64/
version=\$(xpath php-couchbase/package.xml '//package/version/release/text()')
pkg_name="php_couchbase-\${version}-7.3-zts-x64"
mv php_couchbase-7.3.13-zts-x64 \${pkg_name}
zip -r9 \${pkg_name}.zip \${pkg_name}
                """)
                unstash 'php_couchbase-7.3.13-nts-x64'
                sh("""
cp php-couchbase/{package.xml,LICENSE} php_couchbase-7.3.13-nts-x64/
version=\$(xpath php-couchbase/package.xml '//package/version/release/text()')
pkg_name="php_couchbase-\${version}-7.3-nts-x64"
