 set PATH=c:\\php\\php-zts-default-7.3.13-cb1\\msys2\\usr\\bin;%PATH%
 @echo on
 configure.bat --with-couchbase=..\\install\\libcouchbase-windows_msvc2017-amd64 --with-prefix=release
""")
